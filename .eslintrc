{
    "env": {
      "es6": true,
      "jest": true,
      "browser": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "impliedStrict": true,
        "experimentalObjectRestSpread": true
      }
    },
    
    "rules": {
      "react/jsx-indent": [1, 4],
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    }
  }